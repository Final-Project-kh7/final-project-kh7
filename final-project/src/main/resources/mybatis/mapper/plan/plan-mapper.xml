<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="plan">
	
	<insert id="plannerInsert" parameterType="PlannerDto">
		insert into planner values(
			planner_seq.nextval, #{plannerStartDate}, #{plannerEndDate}, 'N', #{plannerName}
		)
	</insert>	
	
	<insert id="placeInsert" parameterType="PlaceDto">
		insert into place values(
			place_seq.nextval, #{placeLatitude}, #{placeLongitude}, #{placeName}, #{placeType}
		)	
	</insert>
	
</mapper>